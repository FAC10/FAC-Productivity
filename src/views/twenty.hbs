{{>appheader}}

<section class="stopwatch">
  <div class="stopwatch__button">
  <svg class="stopwatch__button" id="start" fill="rgb(230,121,121)" height="50" viewBox="0 0 24 24" width="50" xmlns="http://www.w3.org/2000/svg">
    <path d="M8 5v14l11-7z"/>
    <path d="M0 0h24v24H0z" fill="none"/>
</svg>
  <svg class="stopwatch__button" id="pause" fill="rgb(230,121,121)" height="50" viewBox="0 0 24 24" width="50" xmlns="http://www.w3.org/2000/svg">
      <path d="M6 19h4V5H6v14zm8-14v14h4V5h-4z"/>
      <path d="M0 0h24v24H0z" fill="none"/>
  </svg>
  <svg class="stopwatch__button" id="restart" fill="rgb(230,121,121)" height="42" viewBox="0 0 24 24" width="42" xmlns="http://www.w3.org/2000/svg">
      <path d="M0 0h24v24H0z" fill="none"/>
      <path d="M12 5V1L7 6l5 5V7c3.31 0 6 2.69 6 6s-2.69 6-6 6-6-2.69-6-6H4c0 4.42 3.58 8 8 8s8-3.58 8-8-3.58-8-8-8z"/>
  </svg>
  </div>
  <section id="display" class="stopwatch__display">00:00</section>
  <section id="hourdisplay" class="stopwatch__hours"></section>
  <section id="twentycontainer"></section>
</section>
<script type="text/javascript" src="./js/twenty.js"></script>
